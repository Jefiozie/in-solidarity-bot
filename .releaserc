branches:
- "+([0-9])?(.{+([0-9]),x}).x"
- main
- next
- next-major
- name: beta
  prerelease: true
- name: alpha
  prerelease: true
plugins:
  - "@semantic-release/commit-analyzer"
  - "@semantic-release/release-notes-generator"
  - - "@semantic-release/git"
    - assets:
      - docs/**/*
      - package.json
      - package-lock.json
  - "@semantic-release/github"
  - "@semantic-release/npm"
options:
  debug: true
